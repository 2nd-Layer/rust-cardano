    "nodemon": "1.17.1",
    "wasm-loader": "1.3.0",
    "webpack": "3.11.0"
  },
  "peerDependencies": {
    "bip39": "2.3.0"
  },
  "dependencies": {
    "iconv-lite": "0.4.19",
